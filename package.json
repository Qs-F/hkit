{
  "name": "hookskit",
  "version": "0.1.0",
  "description": "Kit for React Hooks",
  "repository": "git@github.com:Qs-F/hookskit.git",
  "author": "Qs-F <qsf@de-liker.com>",
  "license": "MIT",
  "source": "./src/export.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "exports": "./dist/index.modern.js",
  "types": "./dist/export.d.ts",
  "scripts": {
    "prebuild": "yarn clean",
    "build:js": "microbundle --tsconfig tsconfig.build.json",
    "build:d.ts": "tsc-alias -p tsconfig.build.json",
    "build": "run-p build:*",
    "dev:js": "microbundle watch --tsconfig tsconfig.build.json",
    "dev:d.ts": "tsc-alias --watch -p tsconfig.build.json",
    "dev": "run-p dev:*",
    "test": "jest",
    "test:publish:prebuild": "yarn build",
    "test:publish:buildable:install": "yarn --cwd ./examples/basic install",
    "test:publish:buildable:build": "yarn --cwd ./examples/basic build",
    "test:publish": "run-s test:publish:**",
    "clean": "rimraf dist/*",
    "lint:eslint": "eslint --ext .ts,.tsx ./src",
    "lint:tsc": "tsc --noEmit",
    "lint": "run-p lint:*",
    "fmt": "eslint --fix --ext .ts,.tsx ./src"
  },
  "peerDependencies": {
    "@types/react": "^17.0.18",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@jest/globals": "27.4.4",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.14",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "eslint": "8.4.1",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "jest": "27.4.5",
    "microbundle": "0.14.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.1",
    "ts-node": "10.4.0",
    "tsc-alias": "1.4.2",
    "typescript": "4.5.4"
  }
}
