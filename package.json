{
  "name": "hookskit",
  "version": "0.1.0",
  "description": "Kit for React Hooks",
  "repository": "git@github.com:Qs-F/hookskit.git",
  "author": "Qs-F <qsf@de-liker.com>",
  "license": "MIT",
  "source": "./src/export.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "exports": "./dist/index.modern.js",
  "types": "./dist/export.d.ts",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "microbundle --tsconfig tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "microbundle watch --tsconfig tsconfig.build.json & tsc-alias --watch -p tsconfig.build.json",
    "test": "jest",
    "test:publish": "yarn build && cd examples/basic && yarn && yarn build",
    "clean": "rimraf dist/*",
    "lint": "eslint --ext .ts,.tsx ./src && tsc --noEmit",
    "fmt": "eslint --fix --ext .ts,.tsx ./src"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@jest/globals": "27.0.6",
    "@testing-library/react-hooks": "7.0.1",
    "@types/jest": "26.0.24",
    "@types/node": "16.3.3",
    "@types/react": "17.0.15",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "jest": "27.0.6",
    "microbundle": "0.13.3",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "tsc-alias": "1.3.8",
    "typescript": "4.3.5"
  }
}
